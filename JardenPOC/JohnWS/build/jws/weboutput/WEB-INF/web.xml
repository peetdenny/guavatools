<?xml version='1.0' encoding='UTF-8'?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4" id="WebApp_ID">
  <display-name>JohnWS</display-name>
  <servlet>
    <servlet-name>HelloWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.HelloWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>LogMessageWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.LogMessageWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SimpleWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.SimpleWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>LogMessagePlusWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.LogMessagePlusWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>CallbackWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.callback.CallbackWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ShopWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.cart.ShopWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>LogMessageJmsWSServiceServletjms</servlet-name>
    <servlet-class>john.ws.jms.LogMessageJmsWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>HelloJmsWSServiceServletjms</servlet-name>
    <servlet-class>john.ws.jms.HelloJmsWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>LogMessageJmsPersistWSServiceServletjms</servlet-name>
    <servlet-class>john.ws.jms.LogMessageJmsPersistWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>BankWSServiceServlethttp</servlet-name>
    <servlet-class>john.ws.reliable.BankWS</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWSServiceServlethttp</servlet-name>
    <url-pattern>/HelloWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogMessageWSServiceServlethttp</servlet-name>
    <url-pattern>/LogMessageWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SimpleWSServiceServlethttp</servlet-name>
    <url-pattern>/SimpleWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogMessagePlusWSServiceServlethttp</servlet-name>
    <url-pattern>/LogMessagePlusWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CallbackWSServiceServlethttp</servlet-name>
    <url-pattern>/CallbackWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShopWSServiceServlethttp</servlet-name>
    <url-pattern>/ShopWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogMessageJmsWSServiceServletjms</servlet-name>
    <url-pattern>/LogMessageJmsWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HelloJmsWSServiceServletjms</servlet-name>
    <url-pattern>/HelloJmsWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogMessageJmsPersistWSServiceServletjms</servlet-name>
    <url-pattern>/LogMessageJmsPersistWS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BankWSServiceServlethttp</servlet-name>
    <url-pattern>/BankWS</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>
