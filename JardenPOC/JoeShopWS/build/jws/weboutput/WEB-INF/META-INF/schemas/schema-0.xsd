<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="java:joe.ws.cart" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="Invoice">
    <xs:sequence>
      <xs:element name="TotalPrice" type="xs:double" minOccurs="1" nillable="false"/>
      <xs:element name="Client" type="xs:string" minOccurs="1" nillable="true"/>
      <xs:element name="AccountName" type="xs:string" minOccurs="1" nillable="true"/>
      <xs:element name="Items" maxOccurs="unbounded" type="java:CartItem" minOccurs="0" nillable="true" xmlns:java="java:joe.ws.cart"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CartItem">
    <xs:sequence>
      <xs:element name="Code" type="xs:string" minOccurs="1" nillable="true"/>
      <xs:element name="Supplier" type="xs:string" minOccurs="1" nillable="true"/>
      <xs:element name="Name" type="xs:string" minOccurs="1" nillable="true"/>
      <xs:element name="Price" type="xs:double" minOccurs="1" nillable="false"/>
      <xs:element name="Quantity" type="xs:int" minOccurs="1" nillable="false"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>